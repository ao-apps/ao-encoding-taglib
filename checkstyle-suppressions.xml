<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

  <!-- TODO: Complete Javadocs -->
  <suppress checks="(MissingJavadocMethod|SummaryJavadoc)" />

  <!-- Will keep "TEI" suffix for taglib <tei-class> implementations -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\][A-Z][A-Za-z]*TagTEI\.java$"
    message="'[A-Z][A-Za-z]*TagTEI'"
  />

  <!-- Will not reorder overloads -->
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]encoding[/\\]taglib[/\\]EncodingBufferedTag\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]encoding[/\\]taglib[/\\]EncodingFilteredTag\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]encoding[/\\]taglib[/\\]EncodingNullTag\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]encoding[/\\]taglib[/\\]legacy[/\\]EncodingBufferedBodyTag\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]encoding[/\\]taglib[/\\]legacy[/\\]EncodingFilteredBodyTag\.java$"
  />
  <suppress
    checks="OverloadMethodsDeclarationOrder"
    files="[/\\]com[/\\]aoapps[/\\]encoding[/\\]taglib[/\\]legacy[/\\]EncodingNullBodyTag\.java$"
  />

</suppressions>
